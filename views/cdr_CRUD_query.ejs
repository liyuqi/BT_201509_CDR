<% if (resp) { %>
<h3>搜尋結果</h3></br>
<% if (resp.length == 0) { %>
<h4> 查無資料!</h4>
<% } else /*if (resp.length < 20)*/{%>
<h4> 回傳 <%= resp.length %> 筆</h4>

<table class="table table-hover table-condensed">
    <caption></caption>
    <thead>
    <tr>
    <tr>
        <th>Timestamp</th>
        <th>Identifier</th>
        <th>Message</th>
    </tr>
    </thead>
    <tbody>
    <% resp.forEach( function(body){ %>
    <tr>
        <% var key; %>
        <td id="#"><%= i++ %></td>
        <td id="_id"  text="日期時間" ><%= JSON.stringify(body._id) %></td>
        <td id="value"  text="日期時間" ><%= JSON.stringify(body. alue) %></td>
    </tr>
    <% }); %>
    </tbody>
</table>
<% } %>
<% } else { %>
<h3>總量 : <%= totalcount %>筆資料</h3>
<% } %>

